ody]\r\n                    </div>                   \r\n                    <div className=\"field ml-3 \">\r\n                    <label className= \"mr-2 \"> <b>Diarrhea</b></label>\r\n                        {\r\n                            yesno.map((c,i) => \r\n                                <label  className=\" is-small\" key={c}>\r\n                                    <input type=\"radio\" value={c + \", \"} name=\"Diarrhea\" ref={register} />{c + \" \"}\r\n                                </label>\r\n                            )\r\n                        }\r\n                         <p className=\"control \">\r\n                            <input className=\"input is-small\"   ref={register} name=\"Diarrhea_details\" type=\"text\" placeholder=\"Diarrhea details\" />           \r\n                        </p>\r\n                        [onset, frequency, consistency, duration, blood stained, colour (r.g rice water colour), mucoid]\r\n                    </div>   \r\n                    <div className=\"field ml-3 \">\r\n                    <label className= \"mr-2 \"> <b> Nausea</b></label>\r\n                        {\r\n                            yesno.map((c,i) => \r\n                                <label  className=\" is-small\" key={c}>\r\n                                    <input type=\"radio\" value={c + \", \"} name=\"Nausea\" ref={register} />{c + \" \"}\r\n                                </label>\r\n                            )\r\n                        }\r\n                         <p className=\"control \">\r\n                            <input className=\"input is-small\"   ref={register} name=\"Nausea_details\" type=\"text\" placeholder=\"Nausea details\" />           \r\n                        </p>\r\n                    </div> \r\n                    <div className=\"field ml-3 \">\r\n                    <label className= \"mr-2 \"> <b>Vomitting</b></label>\r\n                        {\r\n                            yesno.map((c,i) =